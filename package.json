  "main": "build/back/server.js",
  "scripts": {
    "lint": "eslint .",
    "flow": "flow",
    "image:publish": "yarn image:build-emergency && yarn image:tag && yarn image:push",
    "image:build-emergency": "yarn build && docker build --no-cache -t omnomnom:latest -f ./Dockerfile.emergency .",
    "image:build": "docker build --no-cache -t omnomnom:latest .",
    "image:tag": "docker tag omnomnom gotrgfoodhack/omnomnom:latest",
    "image:push": "docker push gotrgfoodhack/omnomnom:latest",
    "dev:back": "babel-node src/back/server.js",
    "dev:docker": "docker-compose up",
    "dev": "webpack-dev-server",
    "dev:front-local-proxy": "cross-env BACK=http://localhost:3000 webpack-dev-server",
    "dev:front-prod-mode": "cross-env NODE_ENV=production webpack-dev-server",
    "build": "yarn build:back && yarn build:front",
    "build:back": "NODE_ENV=production rollup -c",
    "build:front-local-proxy": "cross-env BACK=http://localhost:3000 webpack",
    "build:front": "NODE_ENV=production webpack"
  },
  "dependencies": {
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "mongoose": "^5.0.7",
