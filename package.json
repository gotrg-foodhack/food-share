    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.66.0",
    "flow-typed": "^2.3.0",
    "glow": "^1.2.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "img-loader": "^2.0.1",
    "jss": "^9.8.0",
    "jss-preset-default": "^4.3.0",
    "mapbox-gl": "^0.44.1",
    "material-ui": "^1.0.0-beta.35",
    "material-ui-icons": "^1.0.0-beta.35",
    "normalize-jss": "^4.0.0",
    "prettier": "^1.11.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-mapbox-gl": "^3.8.0",
    "react-redux": "^5.0.7",
    "recompose": "^0.26.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
